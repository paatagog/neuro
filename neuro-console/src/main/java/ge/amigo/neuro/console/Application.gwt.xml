<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to="gxt">

	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.extjs.gxt.themes.Themes' />
	<inherits name="com.extjs.gxt.ui.GXT"/>

	<!-- only build for Chrome -->
	<!--set-property name="user.agent" value="safari" /-->
	<!-- only build for Firefox -->
	<!--set-property name="user.agent" value="gecko1_8" /-->
	
	<!--Specify the app entry point class.                         -->
	<entry-point class='ge.amigo.neuro.console.client.Application'/>

	<source path='client'/>
	<source path='shared'/>
</module>